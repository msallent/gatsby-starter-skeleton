{
  "name": "gatsby-starter-skeleton",
  "license": "MIT",
  "private": true,
  "version": "2.0.1",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop --host 0.0.0.0",
    "fix": "yarn fix:js && yarn fix:prettier",
    "fix:js": "yarn lint:js --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:prettier",
    "lint:js": "eslint --ignore-path .gitignore . --ext js --ext ts --ext tsx",
    "lint:css": "stylelint --ignore-path .gitignore './src/**/*.ts'",
    "lint:prettier": "prettier --ignore-path .gitignore --check .",
    "serve": "gatsby serve --host 0.0.0.0",
    "type-check": "tsc",
    "plop": "plop"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.12.0",
    "gatsby": "2.32.3",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sharp": "2.14.1",
    "gatsby-plugin-sitemap": "2.12.0",
    "gatsby-plugin-styled-components": "3.10.0",
    "gatsby-plugin-svgr": "2.1.0",
    "gatsby-source-filesystem": "2.11.0",
    "gatsby-transformer-sharp": "2.12.0",
    "normalize.css": "8.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@svgr/webpack": "5.5.0",
    "@types/node": "13.13.47",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.1",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby-plugin-ts-config": "1.1.5",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "plop": "2.7.4",
    "prettier": "2.2.1",
    "stylelint": "13.9.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "post-commit": "git reset"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "eslint --fix",
    "src/**/*.ts": "stylelint"
  }
}
